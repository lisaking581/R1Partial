    <fields>
        <fullName>Amount__c</fullName>
        <externalId>false</externalId>
        <label>Amount</label>
        <precision>9</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invoice_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The first day of support covered by the payment (or first day after the support period for invoices in arrears). The invoice is issued in the previous month or earlier, depending on the contract payment terms)</inlineHelpText>
        <label>Invoice Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Invoice_Number__c</fullName>
        <externalId>false</externalId>
        <label>Invoice Number</label>
        <length>15</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invoiced__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Whether the invoice has been issued. This will be automatically set when the invoice number is filled in</inlineHelpText>
        <label>Invoiced</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Issue_date__c</fullName>
        <externalId>false</externalId>
        <formula>Invoice_Date__c  -   Master_Contract__r.Payment_terms__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The date the invoice should be issued, based on the contract's payment terms</inlineHelpText>
        <label>Issue date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Master_Contract__c</fullName>
        <externalId>false</externalId>
        <label>Master Contract</label>
        <referenceTo>IMX_Contract__c</referenceTo>
        <relationshipLabel>Optima Invoices</relationshipLabel>
        <relationshipName>Optima_Invoices</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
