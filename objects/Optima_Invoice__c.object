        <fullName>Invoice_num_reqd_when_issued</fullName>
        <active>true</active>
        <errorConditionFormula>and( Invoiced__c = TRUE,  ISBLANK( Invoice_Number__c ) )</errorConditionFormula>
        <errorDisplayField>Invoice_Number__c</errorDisplayField>
        <errorMessage>Invoice number must be entered when the invoice has been issued</errorMessage>
