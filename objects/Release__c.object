    <validationRules>
        <fullName>Analyst_required_when_release_planned</fullName>
        <active>true</active>
        <errorConditionFormula>and( ISBLANK( Business_Analyst__c ), not or( ISPICKVAL( Status__c, "Projected" ) ,ISPICKVAL( Status__c, "Tentative" )))</errorConditionFormula>
        <errorDisplayField>Business_Analyst__c</errorDisplayField>
        <errorMessage>Business Analyst must be assigned unless status is Projected or Tentative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivered_status_only_for_some_products</fullName>
        <active>true</active>
        <errorConditionFormula>and( ISPICKVAL( Status__c ,"Delivered") , not or ( ISPICKVAL( Product__c , "Live") , ISPICKVAL( Product__c , "NET")))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Delivered status can only be used for Live or NET. For other products use In Production</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Developer_required_when_release_planned</fullName>
        <active>true</active>
        <errorConditionFormula>and( ISBLANK( Developer__c ), not or( ISPICKVAL( Status__c, "Projected" ) ,ISPICKVAL( Status__c, "Tentative" )))</errorConditionFormula>
        <errorDisplayField>Developer__c</errorDisplayField>
        <errorMessage>Developer must be assigned unless status is Projected or Tentative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Executable_or_data_must_be_selected</fullName>
        <active>true</active>
        <errorConditionFormula>and(Executable__c = false, Data_script__c = false, not ISPICKVAL( Status__c ,"Projected"))</errorConditionFormula>
        <errorDisplayField>Executable__c</errorDisplayField>
        <errorMessage>At least one of Executable or Data/script must be selected for non-Projected releases</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Executable_releases_must_be_32_or_64bit</fullName>
        <active>true</active>
        <errorConditionFormula>and( Executable__c =true,X32_bit__c =false,X64_bit__c =false,not ISPICKVAL( Status__c ,"Projected"),not ISPICKVAL( Status__c ,"Tentative") )</errorConditionFormula>
        <errorDisplayField>X64_bit__c</errorDisplayField>
        <errorMessage>At least one of 32 or 64-bit must be selected for executable releases that are not Projected or Tentative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Full_version_number_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(FIND(".", Version__c, FIND(".", Version__c)+1) = 0, NOT ISPICKVAL(Status__c,"Projected"), NOT ISPICKVAL(Status__c,"Tentative"))</errorConditionFormula>
        <errorDisplayField>Version__c</errorDisplayField>
        <errorMessage>Full version number must be entered for releases that are not Projected or Tentative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Jira_required_when_release_planned</fullName>
        <active>true</active>
        <errorConditionFormula>and( ISBLANK(Jira_Ticket__c), or( ISPICKVAL( Status__c, "Planned" ) ,ISPICKVAL( Status__c, "Tentative" )))</errorConditionFormula>
        <errorDisplayField>Jira_Ticket__c</errorDisplayField>
        <errorMessage>Jira ticket must be filled in if status is Tentative or Planned (enter n/a for implementation releases or patches)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Live_exe_rel_must_have_32_or_64bit_svcs</fullName>
        <active>true</active>
        <description>Live executable releases must have 32- or 64-bit services selected</description>
        <errorConditionFormula>and( Executable__c =true, ISPICKVAL(Product__c,"Live"),  X32_bit_Services__c =false, X64_bit_Services__c =false,not ISPICKVAL( Status__c ,"Projected"),not ISPICKVAL( Status__c ,"Tentative") )</errorConditionFormula>
        <errorDisplayField>X64_bit_Services__c</errorDisplayField>
        <errorMessage>At least one of 32 or 64-bit services must be selected for Live executable releases that are not Projected or Tentative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revision_required_when_delivered</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISBLANK(Revision__c), Executable__c, OR( ISPICKVAL( Status__c, "Delivered" ) ,ISPICKVAL( Status__c, "Not for Production"), ISPICKVAL( Status__c, "In Production"), ISPICKVAL( Status__c, "Superseded")))</errorConditionFormula>
        <errorDisplayField>Revision__c</errorDisplayField>
        <errorMessage>Revision must be filled in for executable releases which have been delivered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Services_only_for_Live</fullName>
        <active>true</active>
        <description>32- and 64-bit services can only be selected for Live releases</description>
        <errorConditionFormula>and(not ISPICKVAL(Product__c,"Live"),  or(X32_bit_Services__c =true, X64_bit_Services__c =true))</errorConditionFormula>
        <errorDisplayField>X64_bit_Services__c</errorDisplayField>
        <errorMessage>32 and/or 64-bit services can only be selected for Live releases</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test_and_release_dates_must_be_different</fullName>
        <active>true</active>
        <errorConditionFormula>and( Release_Date__c = Test_Date__c ,or( ISPICKVAL( Status__c , "Planned") , ISPICKVAL( Status__c , "Tentative") ))</errorConditionFormula>
        <errorDisplayField>Release_Date__c</errorDisplayField>
        <errorMessage>Test date and release date must not be the same for Planned or Tentative releases</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test_date_must_be_before_release_date</fullName>
        <active>true</active>
        <errorConditionFormula>Test_Date__c &gt;  Release_Date__c</errorConditionFormula>
        <errorDisplayField>Test_Date__c</errorDisplayField>
        <errorMessage>Test date must be before release date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tester_required_when_release_planned</fullName>
        <active>true</active>
        <errorConditionFormula>and( ISBLANK(Tester__c ), not or( ISPICKVAL( Status__c, "Projected" ) ,ISPICKVAL( Status__c, "Tentative" )))</errorConditionFormula>
        <errorDisplayField>Tester__c</errorDisplayField>
        <errorMessage>Tester must be assigned unless status is Projected or Tentative</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
