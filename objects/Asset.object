    <fields>
        <fullName>Delivered_Not_Invoiced__c</fullName>
        <externalId>false</externalId>
        <formula>IF (OR(Acctg_Override__c , ABS(Total_Amt_Invoiced__c) &gt;= ABS(Order_Amt__c)),0,
   IF (ABS( Recognized_Amt_3_Acctg__c ) &lt;= ABS(Order_Amt__c),
      (IF (ABS(Total_Amt_Invoiced__c) &gt; ABS(Recognized_Amt_3_Acctg__c), 0, Recognized_Amt_3_Acctg__c - Total_Amt_Invoiced__c)),
             Order_Amt__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Set to 0 if Accounting Override is checked.
Otherwise set to Recognized Amt (Acctg) - Total Amt Invoiced</inlineHelpText>
        <label>Delivered/Not Invoiced</label>
        <precision>18</precision>
