        <fullName>Remaining_Amt_Acctg__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( Acctg_Override__c , 0,
(IF ( AND (ISPICKVAL (Product2.Family, "Travel &amp; Expense"),
OR (ISPICKVAL ( Status, "Completed"), ISPICKVAL ( Status, "Cancelled"))), 0,
IF (ABS(Total_Amt_Invoiced__c) &gt;= ABS( Order_Amt__c ), 0,
Order_Amt__c - Total_Amt_Invoiced__c)
)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
